<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0"
	prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="net.goeller.trackerfx.TrackerFXController">
	<stylesheets>
		<URL value="@TrackerFX.css" />
	</stylesheets>

	<center>
		<ScrollPane BorderPane.alignment="CENTER">
			<content>
				<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0"
					prefWidth="200.0" />
			</content>
		</ScrollPane>
	</center>
	<top>
		<HBox BorderPane.alignment="CENTER">
			<children>
				<TilePane hgap="10.0" prefColumns="3" prefRows="1" vgap="8.0"
					BorderPane.alignment="CENTER">
					<children>
						<Button fx:id="btStop" maxHeight="1.7976931348623157E308"
							maxWidth="1.7976931348623157E308" mnemonicParsing="false"
							onAction="#stop" text="Stop" />
						<Button fx:id="btAdd" maxHeight="1.7976931348623157E308"
							maxWidth="1.7976931348623157E308" mnemonicParsing="false"
							onAction="#add" text="Add" />
						<Button fx:id="btReport" maxHeight="1.7976931348623157E308"
							maxWidth="1.7976931348623157E308" mnemonicParsing="false"
							onAction="#report" text="Report" />
					</children>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</TilePane>
				<AnchorPane HBox.hgrow="ALWAYS">
					<children>
						<Text id="welcome-text" layoutX="14.0" layoutY="35.0"
							strokeType="OUTSIDE" strokeWidth="0.0" text="TrackerFX"
							textAlignment="CENTER" />
					</children>
				</AnchorPane>
			</children>
		</HBox>
	</top>
</BorderPane>
